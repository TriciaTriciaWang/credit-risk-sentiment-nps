    "df = pd.DataFrame(index=range(num_rows))\n",
    "\n",
    "# Create alternating values\n",
    "df['Attrition_Type'] = ['Voluntary' if i % 2 == 0 else 'Involuntary' for i in range(num_rows)]\n",
    "\n",
    "print(df)\n"
